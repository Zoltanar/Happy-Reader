<Window x:Class="Happy_Reader.OutputWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Output" Height="196.093" Width="600" ShowInTaskbar="False" WindowStyle="None" AllowsTransparency="True"  Opacity="0.65" Topmost="True" MouseLeftButtonDown="Window_MouseLeftButtonDown">
    <Window.Background>
        <SolidColorBrush Color="Black"/>
    </Window.Background>
    <Grid>
        <Label x:Name="CharacterLabel" Content="Label" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="White" BorderBrush="#FFB61C1C" Background="{x:Null}" BorderThickness="1" Width="270"/>
        <Label x:Name="ContextLabel" Content="Label" Margin="0,10,10,0" VerticalAlignment="Top" Foreground="White" BorderBrush="#FFB61C1C" Background="{x:Null}" BorderThickness="1" HorizontalAlignment="Right" Width="270" HorizontalContentAlignment="Right"/>
        <DockPanel Name="WebBrowserPanel" Margin="10,43,10,10"/>
    </Grid>
</Window>
